@INPROCEEDINGS{Prost:Sem,
  AUTHOR = {F. Prost},
  TITLE = {On the Semantics of  Non-Interference Type-based Analyses},
  BOOKTITLE = {JFLA'001, Journ\'ees Francophones des Langages Applicatifs},
  MONTH = {January},
  YEAR = {2001},
  ABSTRACT = { Type-based analyses are widely used. In this setting, types are
 decorated with annotations. We address the problem of the semantical
 meaning of annotations. We give an extension of Girard's system $F$
 for non-interference analysis. We give a semantics based on partial
 equivalence relations (PERs) for this extension and show that
 annotations have to be understood as \emph{class} of PERs
 (i.e. trivial and diagonal PERs are two different class of PER). We
 also show that the analysis of a program by type-based systems amount
 to abstract over PER classes.}
}

